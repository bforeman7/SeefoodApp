<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.hulbert.seefood">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ActivityController.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ActivityController.ImageSelectionMenuActivity"></activity>
        <activity android:name="ActivityController.ImageUploadActivity"></activity>
        <activity android:name="ActivityController.SeefoodActivity"></activity>
        <activity android:name="ActivityController.PhotoGalleryActivity"></activity>
        <activity android:name="ActivityController.ImageDetailsActivity"></activity>
    </application>

</manifest>